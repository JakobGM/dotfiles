[submodule "home/.tmux/plugins/tpm"]
	path = home/.tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm
[submodule "archlinux/now_playing"]
	path = archlinux/now_playing
	url = https://github.com/0nse/now_playing.git
    ignore = dirty
[submodule "config/refind/themes"]
	path = config/refind/themes
	url = https://gitlab.com/realmain/rEFInd-sunset.git
[submodule "config/i3/i3scripts"]
	path = config/i3/i3scripts
	url = https://github.com/justbuchanan/i3scripts
[submodule "config/astrality"]
	path = config/astrality
	url = https://github.com/jakobgm/astrality
[submodule "config/kitty/kitty_search"]
	path = config/kitty/kitty_search
	url = https://github.com/trygveaa/kitty-kitten-search
